        "properties": {}
      },
      "response_user_delete": {
        "type": "object",
        "properties": {}
      },
      "response_association_delete": {
        "type": "object",
        "properties": {}
      },
      "associations_info": {
        "type": "object",
        "properties": {}
      },
      "qos_info": {
