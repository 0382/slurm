        "properties": {}
      },
      "response_tres": {
        "type": "object",
        "properties": {}
      },
      "tres_info": {
        "type": "object",
        "properties": {}
      },
      "job_info": {
        "type": "object",
        "properties": {}
      },
      "config_info": {
        "type": "object",
        "properties": {}
      },
      "config_response": {
        "type": "object",
        "properties": {}
